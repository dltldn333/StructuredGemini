{
  "manifest_version": 3,
  "name": "Structured Gemini",
  "version": "1.0",
  "description": "Typed grouping for Gemini",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://gemini.google.com/*"],
      "js": ["dist/main.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
